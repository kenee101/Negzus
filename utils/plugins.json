{
    "plugins":
    [
        [
        "expo-notifications",
        {
        "icon": "./assets/images/notification-icon.png",
        "color": "#000000",
        "sounds": [
            {
            "name": "notification-sound",
            "filename": "./assets/sounds/notification-sound.mp3"
            }
        ]
        }
        ],
        [
        "expo-camera",
        {
        "cameraPermission": "Allow $(name) to use your camera.",
        "microphonePermission": "Allow $(name) to access your microphone",
        "recordAudioAndroid": true
        }
        ],
        [
        "expo-image-picker",
        {
        "photosPermission": "Allow $(name) to access your photos."
        }
        ],
        [
        "expo-contacts",
        {
        "contactsPermission": "Allow $(name) to access your contacts."
        }
        ],
        [
        "expo-keep-awake",
        {
        "keepAwakePermission": "Allow $(name) to keep your screen awake."
        }
        ],
        [
        "expo-ads-admob",
        {
        "appId": "ca-app-pub-3940256099942544~3347511713"
        }
        ],
        [
        "expo-application",
        {
        "appOwnership": "standalone"
        }
        ],
        [
        "expo-permissions",
        {
        "permissions": ["camera", "contacts", "location"]
        }
        ]
    ]
}